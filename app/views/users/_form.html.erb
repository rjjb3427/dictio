<fieldset class="form-group">
  <%= form.label :email, class: "lead" %><br>
  <%= form.text_field :email, type: 'email', placeholder: 'Required', class: "form-control" %>
  
  <%= form.label :first_name, class: "lead" %> <br>
  <%= form.text_field :first_name, placeholder: 'Optional', class: "form-control" %>
  
  <%= form.label :last_initial, class: "lead" %> <br>
  <%= form.text_field :last_initial, placeholder: 'Optional', class: "form-control" %>
  
</fieldset>
<fieldset class="form-group">
  <%= form.label :password, class: "lead" %><br>
  <%= form.password_field :password, placeholder: 'Required', class: "form-control" %>
</fieldset>

