<fieldset class="form-group">
  <%= form.label :email, class: "lead" %><br>
  <%= form.text_field :email, type: 'email', placeholder: 'Required', class: "form-control" %>

  <%= form.label :username, class: "lead" %> <br>
  <%= form.text_field :username, placeholder: 'Required', class: "form-control" %>
</fieldset>
<fieldset class="form-group">
  <%= form.label :password, class: "lead" %><br>
  <%= form.password_field :password, placeholder: 'Required', class: "form-control" %>
</fieldset>

